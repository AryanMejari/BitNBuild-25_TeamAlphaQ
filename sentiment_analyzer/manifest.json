{
  "manifest_version": 3,
  "name": "SA",
  "version": "1.0.0",
  "description": "A React app as a Chrome extension",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage",
    "tabs"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png"
  }
}
